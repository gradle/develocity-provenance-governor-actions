{
  "name": "attest-action",
  "description": "Example action that creates attestations",
  "version": "0.1.0",
  "author": "",
  "type": "module",
  "private": true,
  "homepage": "",
  "keywords": [
    "actions"
  ],
  "exports": {
    ".": "./dist/index.js"
  },
  "engines": {
    "node": ">=20"
  },
  "scripts": {
    "format:write": "npx prettier --write .",
    "format:check": "npx prettier --check .",
    "lint": "npx eslint .",
    "local-action": "npx @github/local-action . src/publish/main.ts .env",
    "prepackage": "npm run format:check && npm run lint",
    "package": "npx rollup --config rollup.config.ts --configPlugin @rollup/plugin-typescript",
    "package:watch": "npm run package -- --watch",
    "test": "NODE_OPTIONS=\"$NODE_OPTIONS --experimental-vm-modules\" NODE_NO_WARNINGS=1 npx jest",
    "build": "npm run format:write && npm run lint && npm run test",
    "build-all": "npm run build && npm run package",
    "render": "node --loader ts-node/esm script/render.js"
  },
  "dependencies": {
    "@actions/core": "^1.11.1",
    "packageurl-js": "^2.0.1"
  },
  "devDependencies": {
    "@eslint/compat": "^1.4.0",
    "@github/local-action": "^6.0.2",
    "@gradle-tech/develocity-agent": "https://develocity-npm-pkgs.gradle.com/gradle-develocity-agent-2.0.0-rc-1.tgz",
    "@jest/globals": "^30.2.0",
    "@rollup/plugin-commonjs": "29.0.0",
    "@rollup/plugin-node-resolve": "^16.0.3",
    "@rollup/plugin-typescript": "^12.3.0",
    "@types/jest": "^30.0.0",
    "@types/node": "^20.19.23",
    "@typescript-eslint/eslint-plugin": "^8.46.2",
    "@typescript-eslint/parser": "^8.32.1",
    "eslint": "^9.38.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jest": "^29.0.1",
    "eslint-plugin-prettier": "^5.5.4",
    "jest": "^30.2.0",
    "make-coverage-badge": "^1.2.0",
    "marked": "^16.4.1",
    "prettier": "^3.6.2",
    "prettier-eslint": "^16.4.2",
    "rollup": "^4.52.5",
    "ts-jest": "^29.4.5",
    "ts-jest-resolver": "^2.0.1",
    "typescript": "^5.9.3"
  },
  "optionalDependencies": {
    "@rollup/rollup-linux-x64-gnu": "*"
  }
}
